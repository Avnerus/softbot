    "hybrids": "^4.3.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nipplejs": "^0.7.1",
    "node-fetch": "^2.6.6",
    "node-google-translate-skidz": "^1.1.2",
    "ogg": "^1.2.6",
    "reconnecting-websocket": "^4.4.0",
    "recordrtc": "^5.6.2",
    "redux": "^4.1.2",
    "stats.js": "^0.17.0",
    "svg-country-flags": "^1.2.10",
    "tmp": "^0.1.0",
