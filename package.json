    "emoji-replace": "^1.0.1",
    "express": "^4.14.0",
    "fullscreen": "^1.1.0",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "nan": "^2.11.1",
    "nipplejs": "^0.7.1",
    "node-google-translate-skidz": "^1.1.2",
    "reconnecting-websocket": "^4.1.10",
    "recordrtc": "^5.4.1",
