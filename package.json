{
  "name": "softbot",
  "version": "1.0.0",
  "description": "Softbot",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/index.js --exec babel-node",
    "serve": "webpack-dev-server --host 0.0.0.0",
    "control": "cargo run --manifest-path softcontrol/rust/soft_control/Cargo.toml",
    "wasm": "cargo +nightly build --manifest-path softcontrol/rust/wasm/Cargo.toml --lib --target wasm32-unknown-unknown &&  wasm-bindgen softcontrol/rust/wasm/target/wasm32-unknown-unknown/debug/breakout.wasm --out-dir client/common/breakout"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avnerus/divine.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Avnerus/divine/issues"
  },
  "homepage": "https://github.com/Avnerus/divine#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  },
  "window": {
    "title": "Softbot",
    "toolbar": false,
    "frame": false,
    "width": 1920,
    "height": 1080,
    "position": "mouse",
    "fullscreen": true
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@google-cloud/speech": "^2.3.1",
    "@google-cloud/text-to-speech": "^0.5.1",
    "@google-cloud/translate": "^2.1.4",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "body-parser": "^1.18.3",
    "emoji-replace": "^1.0.1",
    "express": "^4.14.0",
    "formidable": "^1.2.1",
    "fullscreen": "^1.1.0",
    "hybrids": "^4.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "nipplejs": "^0.7.1",
    "node-fetch": "^2.3.0",
    "node-google-translate-skidz": "^1.1.2",
    "node-opus": "^0.3.1",
    "ogg": "^1.2.6",
    "reconnecting-websocket": "^4.1.10",
    "recordrtc": "^5.4.1",
    "stats.js": "^0.17.0",
    "tone": "^13.4.9",
    "typed.js": "^2.0.2",
    "wav": "^1.0.2",
    "xmlbuilder": "^11.0.0",
    "youtube-iframe": "^1.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  }
}
